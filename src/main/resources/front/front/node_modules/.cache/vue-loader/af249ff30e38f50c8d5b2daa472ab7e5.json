>\r\n      <template #eventContent=\"{ event }\">\r\n        <div>\r\n          <strong>{{ event.title }}</strong>\r\n        </div>\r\n      </template>\r\n    </FullCalendar>\r\n\r\n    <!-- 添加待办事项表单 -->\r\n    <form @submit.prevent=\"addTodo\">\r\n      <input v-model=\"newTodo.title\" placeholder=\"输入待办事项标题\" required />\r\n      <input v-model=\"newTodo.date\" type=\"date\" required />\r\n      <button type=\"submit\">添加待办事项</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport FullCalendar from \"@fullcalendar/vue3\";\r\nimport dayGridPlugin from \"@fullcalendar/daygrid\";\r\nimport interactionPlugin from \"@fullcalendar/interaction\";\r\n\r\nexport default {\r\n  components: {\r\n    FullCalendar,\r\n  },\r\n  data() {\r\n    return {\r\n      // 待办事项列表\r\n      todos: [\r\n        { title: \"完成报告\", date: \"2025-04-18\" },\r\n        { title: \"团队会议\", date: \"2025-04-20\" },\r\n      ],\r\n      // 新待办事项\r\n      newTodo: {\r\n        title: \"\",\r\n        date: \"\",\r\n      },\r\n      // 日历配置\r\n      calendarOptions: {\r\n        plugins: [dayGridPlugin, interactionPlugin],\r\n        initialView: \"dayGridMonth\",\r\n        events: [], // 初始事件为空\r\n        dateClick: this.handleDateClick,\r\n      },\r\n    };\r\n  },\r\n  mounted() {\r\n    // 初始化日历事件\r\n    this.calendarOptions.events = this.todos.map((todo) => ({\r\n      title: todo.title,\r\n      date: todo.date,\r\n    }));\r\n  },\r\n  methods: {\r\n    // 处理日期点击\r\n    handleDateClick(arg) {\r\n      this.newTodo.date = arg.dateStr; // 自动填充日期\r\n    },\r\n    // 添加待办事项\r\n    addTodo() {\r\n      if (this.newTodo.title && this.newTodo.date) {\r\n        this.todos.push({ ...this.newTodo });\r\n        this.calendarOptions.events.push({\r\n          title: this.newTodo.title,\r\n          date: this.newTodo.date,\r\n        });\r\n        this.newTodo = { title: \"\", date: \"\" }; // 清空输入框\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n/* 引入 FullCalendar 样式 */\r\n@import \"~@fullcalendar/core/main.css\";\r\n@import \"~@fullcalendar/daygrid/main.css\";\r\n\r\nform {\r\n  margin-top: 20px;\r\n}\r\n\r\ninput {\r\n  margin-right: 10px;\r\n}\r\n</style>\r\n"]}]}