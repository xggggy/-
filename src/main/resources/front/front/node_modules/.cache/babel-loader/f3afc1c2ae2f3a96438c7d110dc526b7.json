lStorage.getItem('tips'),\n                          type: 'success',\n                          duration: 1500,\n                         });\n                          return false;\n                     } else {\n                         // 跨表计算\n\n\n                          this.$http.post('jiaofeixinxi/add', this.ruleForm).then(res => {\n                              if (res.data.code == 0) {\n                                  this.$message({\n                                      message: '操作成功',\n                                      type: 'success',\n                                      duration: 1500,\n                                      onClose: () => {\n                                          this.$router.go(-1);\n                                      }\n                                  });\n                              } else {\n                                  this.$message({\n                                      message: res.data.msg,\n                                      type: 'error',\n                                      duration: 1500\n                                  });\n                              }\n                          });\n                     }\n                 });\n             } else {\n\n\n                  this.$http.post('jiaofeixinxi/add', this.ruleForm).then(res => {\n                     if (res.data.code == 0) {\n                          this.$message({\n                              message: '操作成功',\n                              type: 'success',\n                              duration: 1500,\n                              onClose: () => {\n                                  this.$router.go(-1);\n                              }\n                          });\n                      } else {\n                          this.$message({\n                              message: res.data.msg,\n                              type: 'error',\n                              duration: 1500\n                          });\n    