shizhengleixing : false,\r\n            chepaihaoma : false,\r\n            cheliangzhuangkuang : false,\r\n            yuyueshijian : false,\r\n            jiaolianzhanghao : false,\r\n            jiaolianxingming : false,\r\n            xueyuanzhanghao : false,\r\n            xueyuanxingming : false,\r\n            shouji : false,\r\n            dengjishijian : false,\r\n            zhuangtai : false,\r\n        },\r\n        type: '',\r\n        userTableName: localStorage.getItem('UserTableName'),\r\n        ruleForm: {\r\n          yuyuehao: this.getUUID(),\r\n          cheliangmingcheng: '',\r\n          cheliangtupian: '',\r\n          jiashizhengleixing: '',\r\n          chepaihaoma: '',\r\n          cheliangzhuangkuang: '',\r\n          yuyueshijian: '',\r\n          jiaolianzhanghao: '',\r\n          jiaolianxingming: '',\r\n          xueyuanzhanghao: '',\r\n          xueyuanxingming: '',\r\n          shouji: '',\r\n          dengjishijian: '',\r\n          zhuangtai: '正常',\r\n        },\r\n        zhuangtaiOptions: [],\r\n        rules: {\r\n          yuyuehao: [\r\n          ],\r\n          cheliangmingcheng: [\r\n            { required: true, message: '车辆名称不能为空', trigger: 'blur' },\r\n          ],\r\n          cheliangtupian: [\r\n          ],\r\n          jiashizhengleixing: [\r\n          ],\r\n          chepaihaoma: [\r\n            { required: true, message: '车牌号码不能为空', trigger: 'blur' },\r\n          ],\r\n          cheliangzhuangkuang: [\r\n          ],\r\n          yuyueshijian: [\r\n            { required: true, message: '预约时间不能为空', trigger: 'blur' },\r\n          ],\r\n          jiaolianzhanghao: [\r\n          ],\r\n          jiaolianxingming: [\r\n          ],\r\n          xueyuanzhanghao: [\r\n          ],\r\n          xueyuanxingming: [\r\n          ],\r\n          shouji: [\r\n            { validator: this.$validate.isMobile, trigger: 'blur' },\r\n          ],\r\n          dengjishijian: [\r\n          ],\r\n          zhuangtai: [\r\n     